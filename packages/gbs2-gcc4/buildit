#mkdir -p gcc/doc
#touch gcc/doc/cpp.info
#touch gcc/doc/gcc.info
#touch gcc/doc/gccint.info
#touch gcc/doc/cppinternals.info

#make -j $DIDBS_JOBS || exit -1

exit 0

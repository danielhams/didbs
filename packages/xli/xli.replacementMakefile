all:: $(PROGRAMS)

xli: $(OBJS1) $(DEPLIBS1)
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS) $(OBJS1) $(DEFINES) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xli
	 -@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xli $(DESTDIR)$(BINDIR)/xli

xli.nr.z : xli.man
	@if [ -f xli.man ]; then \
		$(RM) xli.nr xli.nr.z && \
		tbl xli.man | eqn | nroff -man - > xli.nr && \
		pack -f xli.nr; \
	fi

install.man:: xli.nr.z
	 -@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi; \
	if [ -f xli.man ]; then \
		$(INSTALL) -c $(INSTMANFLAGS) \
			xli.nr.z $(DESTDIR)$(MANDIR)/xli.z; \
	fi

clean::
	$(RM) xli.nr xli.nr.z

depend::
	 $(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	 $(RM) $(PROGRAMS)

xlito: $(OBJS2) $(DEPLIBS2)
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS) $(OBJS2)  $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xlito
	 -@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xlito $(DESTDIR)$(BINDIR)/xlito

xlito.nr.z : xlito.man
	@if [ -f xlito.man ]; then \
		$(RM) xlito.nr xlito.nr.z && \
		tbl xlito.man | eqn | nroff -man - > xlito.nr && \
		pack -f xlito.nr; \
	fi

install.man:: xlito.nr.z
	 -@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi; \
	if [ -f xlito.man ]; then \
		$(INSTALL) -c $(INSTMANFLAGS) \
			xlito.nr.z $(DESTDIR)$(MANDIR)/xlito.z; \
	fi

clean::
	$(RM) xlito.nr xlito.nr.z

install:: $(SYSPATHFILE)
#	$(RM) $(BINDIR)/xview $(BINDIR)/xsetbg
	$(LN) $(BINDIR)/xli $(BINDIR)/xview
	$(LN) $(BINDIR)/xli $(BINDIR)/xsetbg

$(SYSPATHFILE):
	@echo "*** Creating default $(SYSPATHFILE) since you"
	@echo "*** don't have one. This file is used to set up default places"
	@echo "*** and names to look for images.  You probably want to edit"
	@echo "*** it for your site.  See the xloadimage manual page for"
	@echo "*** details on the contents of this file."
	cp /dev/null $(SYSPATHFILE)
	echo "path=$(INCDIR)/bitmaps $(INCDIR)/images" \
		>> $(SYSPATHFILE)
	echo "extension=.gif .jpg .rle .csun .msun .sun .face .xbm .bm" \
		>>$(SYSPATHFILE)
	chmod a+r $(SYSPATHFILE)


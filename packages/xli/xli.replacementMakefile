PROGRAMS = xli xlito

  OBJS = $(OBJS1) $(OBJS2) $(OBJS3) $(OBJS4) $(OBJS5) \
         $(OBJS6) $(OBJS7) $(OBJS8) $(OBJS9) $(OBJS10)
